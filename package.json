  "bugs": {
    "url": "https://github.com/OussamaGhamian/financial_tracker_larvel/issues"
  },
  "homepage": "https://github.com/OussamaGhamian/financial_tracker_larvel#readme",
  "dependencies": {
    "concurrently": "^5.1.0"
  }
}
